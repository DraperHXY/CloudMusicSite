<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>DownloadMusic</servlet-name>
    <servlet-class>com.draper.servlet.DownloadMusicServlet</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>SignUpServlet</servlet-name>
    <servlet-class>com.draper.servlet.SignUpServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>DownloadMusic</servlet-name>
    <url-pattern>/DreamingAlone.mp3</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>SignUpServlet</servlet-name>
    <url-pattern>/SignUp.do</url-pattern>
  </servlet-mapping>

  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>

</web-app>
